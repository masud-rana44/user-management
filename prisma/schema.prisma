generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id     String @id @default(uuid()) @map("_id")
  name   String
  email  String
  gender String
  status String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
